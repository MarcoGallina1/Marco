<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend_color" name="toperp_custom_view assets backend" inherit_id="web.assets_backend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/toperp_custom_view/static/src/scss/custom_style.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/toperp_custom_view/static/src/js/views/limit_views.js"/>
        </xpath>
    </template>
</odoo>
